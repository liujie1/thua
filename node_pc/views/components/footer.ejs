<%if(trust_timeout_dialog_show == "true"){%>
  <div class="dialog dialog-alert" id="global_dialog" is-show="true">
      <h1 class="title">温馨提示：</h1>
      <div class="container">
          <p>
              当前登录已过期，请重新登录
          </p>
      </div>
      <a class="button" href="<%= constant.bank_home_url%>">
        确定
      </a>
      <iframe style="z-index:-100;border:1px;position:absolute;top:0;left:0;width:100%;height:100%;"></iframe>
  </div>
