                    <tr>
                        <td width="14%" height="40" align="right" class="STYLE2" id="login">
                            验证码：
                        </td>
                        <td width="25%" height="40">
                            <input class="verify-input" type="text" id="captcha" data-link="captcha" name="captcha" placeholder="验证码" required number="number"
