        <!--注册成功-->
        <div class="row-container info-page">
            <form method="post" action="<%= constant.base_url%>/test">

                <table>
                    <tr>
                        <td width="14%" height="40" align="right" class="">
                            用户名：
                        </td>
                        <td width="25%" height="40">
                            <input type="text" name="custlogname" id="username" tabindex="1" class="" value="13111111111" />
